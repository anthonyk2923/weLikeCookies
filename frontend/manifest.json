{
    "manifest_version": 3,
    "name": "Multi-Tab Cookie Access",
    "version": "1.0",
    "permissions": [
      "cookies",
      "tabs",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  